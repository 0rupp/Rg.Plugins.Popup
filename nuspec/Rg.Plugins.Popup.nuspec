        <version>0.0.14-pre1</version>
        <title>Popup Page Plugin for Xamarin Forms</title>
        <authors>Kirill Lyubimov</authors>
        <owners>rotorgames</owners>
        <projectUrl>https://github.com/rotorgames/Rg.Plugins.Popup</projectUrl>
